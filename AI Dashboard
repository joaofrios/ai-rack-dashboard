        // Tab switching functionality
        function showTab(tabName) {
            // Hide all tab contents
            document.querySelectorAll('.tab-content').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Remove active class from all nav tabs
            document.querySelectorAll('.nav-tab').forEach(tab => {
                tab.classList.remove('active');
            });
            
            // Show selected tab content
            document.getElementById(tabName).classList.add('active');
            
            // Add active class to clicked nav tab
            event.target.classList.add('active');
            
            // Initialize charts for the active tab
            setTimeout(() => {
                initializeChartsForTab(tabName);
            }, 100);
        }

        function initializeChartsForTab(tabName) {
            switch(tabName) {
                case 'overview':
                    createOpportunityChart();
                    createCategoryChart();
                    break;
                case 'financial':
                    createPriceChart();
                    createVolatilityChart();
                    populateGrowthLeadersTable();
                    break;
                case 'opportunities':
                    populateOpportunityRankingTable();
                    populateExpansionIndicators();
                    populateMarketTrends();
                    break;
                case 'recommendations':
                    populateStrategicRecommendations();
                    createRevenueProjectionChart();
                    break;
                case 'companies':
                    populateCompanyProfilesTable();
                    break;
            }
        }

        // Chart creation functions
        function createOpportunityChart() {
            const ctx = document.getElementById('opportunityChart');
            if (!ctx) return;
            
            // Destroy existing chart if it exists
            if (window.opportunityChartInstance) {
                window.opportunityChartInstance.destroy();
            }
            
            const data = {
                datasets: [{
                    label: 'Companies',
                    data: companiesData.map(company => ({
                        x: company.logistics_opportunity_score,
                        y: company.price_change_1y,
                        label: company.symbol,
                        category: company.category
                    })),
                    backgroundColor: companiesData.map(company => {
                        if (company.category === 'Semiconductor Designer') return 'rgba(139, 92, 246, 0.7)';
                        if (company.category === 'Cloud Provider') return 'rgba(59, 130, 246, 0.7)';
                        if (company.category === 'Foundry') return 'rgba(245, 158, 11, 0.7)';
                        if (company.category === 'Equipment') return 'rgba(16, 185, 129, 0.7)';
                        if (company.category === 'Memory') return 'rgba(236, 72, 153, 0.7)';
                        return 'rgba(107, 114, 128, 0.7)';
                    }),
                    borderColor: companiesData.map(company => {
                        if (company.category === 'Semiconductor Designer') return 'rgba(139, 92, 246, 1)';
                        if (company.category === 'Cloud Provider') return 'rgba(59, 130, 246, 1)';
                        if (company.category === 'Foundry') return 'rgba(245, 158, 11, 1)';
                        if (company.category === 'Equipment') return 'rgba(16, 185, 129, 1)';
                        if (company.category === 'Memory') return 'rgba(236, 72, 153, 1)';
                        return 'rgba(107, 114, 128, 1)';
                    }),
                    pointRadius: 8,
                    pointHoverRadius: 12
                }]
            };

            window.opportunityChartInstance = new Chart(ctx, {
                type: 'scatter',
                data: data,
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        },
                        tooltip: {
                            callbacks: {
                                label: function(context) {
                                    const point = context.raw;
                                    return `${point.label}: Score ${point.x}, Growth ${point.y.toFixed(1)}%`;
                                }
                            }
                        }
                    },
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Logistics Opportunity Score'
                            },
                            min: 30,
                            max: 75
                        },
                        y: {
                            title: {
                                display: true,
                                text: '1-Year Growth (%)'
                            }
                        }
                    }
                }
            });
        }

        function createCategoryChart() {
            const ctx = document.getElementById('categoryChart');
            if (!ctx) return;
            
            if (window.categoryChartInstance) {
                window.categoryChartInstance.destroy();
            }
            
            // Calculate category averages
            const categories = {};
            companiesData.forEach(company => {
                if (!categories[company.category]) {
                    categories[company.category] = {
                        scores: [],
                        growth: []
                    };
                }
                categories[company.category].scores.push(company.logistics_opportunity_score);
                categories[company.category].growth.push(company.price_change_1y);
            });
            
            const categoryNames = Object.keys(categories);
            const avgScores = categoryNames.map(cat => 
                categories[cat].scores.reduce((a, b) => a + b, 0) / categories[cat].scores.length
            );
            const avgGrowth = categoryNames.map(cat => 
                categories[cat].growth.reduce((a, b) => a + b, 0) / categories[cat].growth.length
            );

            window.categoryChartInstance = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: categoryNames,
                    datasets: [{
                        label: 'Avg Opportunity Score',
                        data: avgScores,
                        backgroundColor: 'rgba(59, 130, 246, 0.7)',
                        borderColor: 'rgba(59, 130, 246, 1)',
                        borderWidth: 2,
                        yAxisID: 'y'
                    }, {
                        label: 'Avg Growth (%)',
                        data: avgGrowth,
                        backgroundColor: 'rgba(16, 185, 129, 0.7)',
                        borderColor: 'rgba(16, 185, 129, 1)',
                        borderWidth: 2,
                        yAxisID: 'y1'
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    scales: {
                        y: {
                            type: 'linear',
                            display: true,
                            position: 'left',
                            title: {
                                display: true,
                                text: 'Opportunity Score'
                            }
                        },
                        y1: {
                            type: 'linear',
                            display: true,
                            position: 'right',
                            title: {
                                display: true,
                                text: 'Growth (%)'
                            },
                            grid: {
                                drawOnChartArea: false,
                            },
                        }
                    }
                }
            });
        }

        function createPriceChart() {
            const ctx = document.getElementById('priceChart');
            if (!ctx) return;
            
            if (window.priceChartInstance) {
                window.priceChartInstance.destroy();
            }
            
            const sortedData = [...companiesData].sort((a, b) => b.price_change_1y - a.price_change_1y);

            window.priceChartInstance = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: sortedData.map(company => company.symbol),
                    datasets: [{
                        label: '1-Year Growth (%)',
                        data: sortedData.map(company => company.price_change_1y),
                        backgroundColor: sortedData.map(company => 
                            company.price_change_1y > 0 ? 'rgba(16, 185, 129, 0.7)' : 'rgba(239, 68, 68, 0.7)'
                        ),
                        borderColor: sortedData.map(company => 
                            company.price_change_1y > 0 ? 'rgba(16, 185, 129, 1)' : 'rgba(239, 68, 68, 1)'
                        ),
                        borderWidth: 2
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        y: {
                            title: {
                                display: true,
                                text: 'Growth Percentage (%)'
                            }
                        }
                    }
                }
            });
        }

        function createVolatilityChart() {
            const ctx = document.getElementById('volatilityChart');
            if (!ctx) return;
            
            if (window.volatilityChartInstance) {
                window.volatilityChartInstance.destroy();
            }
            
            const sortedData = [...companiesData].sort((a, b) => a.volatility_30d - b.volatility_30d);

            window.volatilityChartInstance = new Chart(ctx, {
                type: 'horizontalBar',
                data: {
                    labels: sortedData.map(company => company.symbol),
                    datasets: [{
                        label: '30-Day Volatility (%)',
                        data: sortedData.map(company => company.volatility_30d),
                        backgroundColor: sortedData.map(company => {
                            if (company.volatility_30d < 25) return 'rgba(16, 185, 129, 0.7)';
                            if (company.volatility_30d < 40) return 'rgba(245, 158, 11, 0.7)';
                            return 'rgba(239, 68, 68, 0.7)';
                        }),
                        borderColor: sortedData.map(company => {
                            if (company.volatility_30d < 25) return 'rgba(16, 185, 129, 1)';
                            if (company.volatility_30d < 40) return 'rgba(245, 158, 11, 1)';
                            return 'rgba(239, 68, 68, 1)';
                        }),
                        borderWidth: 2
                    }]
                },
                options: {
                    indexAxis: 'y',
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            display: false
                        }
                    },
                    scales: {
                        x: {
                            title: {
                                display: true,
                                text: 'Volatility (%)'
                            }
                        }
                    }
                }
            });
        }

        function createRevenueProjectionChart() {
            const ctx = document.getElementById('revenueProjectionChart');
            if (!ctx) return;
            
            if (window.revenueChartInstance) {
                window.revenueChartInstance.destroy();
            }
            
            // Estimate revenue opportunities based on opportunity scores
            const revenueData = companiesData.slice(0, 8).map(company => ({
                symbol: company.symbol,
                estimated_revenue: (company.logistics_opportunity_score * 2.5) + (company.price_change_1y * 0.5)
            }));

            window.revenueChartInstance = new Chart(ctx, {
                type: 'doughnut',
                data: {
                    labels: revenueData.map(item => item.symbol),
                    datasets: [{
                        data: revenueData.map(item => item.estimated_revenue),
                        backgroundColor: [
                            'rgba(139, 92, 246, 0.8)',
                            'rgba(59, 130, 246, 0.8)',
                            'rgba(245, 158, 11, 0.8)',
                            'rgba(16, 185, 129, 0.8)',
                            'rgba(236, 72, 153, 0.8)',
                            'rgba(107, 114, 128, 0.8)',
                            'rgba(239, 68, 68, 0.8)',
                            'rgba(34, 197, 94, 0.8)'
                        ],
                        borderWidth: 2
                    }]
                },
                options: {
                    responsive: true,
                    maintainAspectRatio: false,
                    plugins: {
                        legend: {
                            position: 'bottom'
                        }
                    }
                }
            });
        }

        // Table population functions
        function populateGrowthLeadersTable() {
            const tbody = document.getElementById('growthLeadersTable');
            if (!tbody) return;
            
            const topGrowth = [...companiesData]
                .sort((a, b) => b.price_change_1y - a.price_change_1y)
                .slice(0, 8);
            
            tbody.innerHTML = topGrowth.map(company => `
                <tr>
                    <td><span class="company-symbol">${company.symbol}</span><br><small>${company.company_name}</small></td>
                    <td><span class="growth-indicator ${company.price_change_1y > 0 ? 'growth-up' : 'growth-down'}">${company.price_change_1y > 0 ? '+' : ''}${company.price_change_1y.toFixed(1)}%</span></td>
                    <td>$${company.current_price.toFixed(2)}</td>
                    <td><span class="category-badge ${getCategoryClass(company.category)}">${company.category}</span></td>
                </tr>
            `).join('');
        }

        function populateOpportunityRankingTable() {
            const tbody = document.getElementById('opportunityRankingTable');
            if (!tbody) return;
            
            const sortedOpportunities = [...companiesData]
                .sort((a, b) => b.logistics_opportunity_score - a.logistics_opportunity_score);
            
            tbody.innerHTML = sortedOpportunities.map((company, index) => `
                <tr>
                    <td><strong>${index + 1}</strong></td>
                    <td><span class="company-symbol">${company.symbol}</span><br><small>${company.company_name}</small></td>
                    <td><span class="opportunity-score ${getScoreClass(company.logistics_opportunity_score)}">${company.logistics_opportunity_score}/100</span></td>
                    <td><span class="growth-indicator ${getTrajectoryClass(company.growth_trajectory)}">${company.growth_trajectory}</span></td>
                    <td><span class="growth-indicator ${company.price_change_1y > 0 ? 'growth-up' : 'growth-down'}">${company.price_change_1y > 0 ? '+' : ''}${company.price_change_1y.toFixed(1)}%</span></td>
                    <td><span class="category-badge ${getCategoryClass(company.category)}">${company.category}</span></td>
                </tr>
            `).join('');
        }

        function populateCompanyProfilesTable() {
            const tbody = document.getElementById('companyProfilesTable');
            if (!tbody) return;
            
            tbody.innerHTML = companiesData.map(company => `
                <tr>
                    <td><span class="company-symbol">${company.symbol}</span></td>
                    <td>${company.company_name}</td>
                    <td>$${company.current_price.toFixed(2)}</td>
                    <td><span class="growth-indicator ${company.price_change_1y > 0 ? 'growth-up' : 'growth-down'}">${company.price_change_1y > 0 ? '+' : ''}${company.price_change_1y.toFixed(1)}%</span></td>
                    <td><span class="growth-indicator ${company.price_change_3m > 0 ? 'growth-up' : 'growth-down'}">${company.price_change_3m > 0 ? '+' : ''}${company.price_change_3m.toFixed(1)}%</span></td>
                    <td>${company.volatility_30d.toFixed(1)}%</td>
                    <td><span class="opportunity-score ${getScoreClass(company.logistics_opportunity_score)}">${company.logistics_opportunity_score}/100</span></td>
                    <td><span class="category-badge ${getCategoryClass(company.category)}">${company.category}</span></td>
                </tr>
            `).join('');
        }

        function populateExpansionIndicators() {
            const container = document.getElementById('expansionIndicators');
            if (!container) return;
            
            const highGrowthCompanies = companiesData.filter(c => c.price_change_1y > 30);
            const momentumCompanies = companiesData.filter(c => c.price_change_3m > 20);
            
            container.innerHTML = `
                <div class="recommendation-card rec-immediate">
                    <div class="rec-title"> High Growth Companies (>30% 1Y)</div>
                    <ul class="rec-list">
                        ${highGrowthCompanies.map(c => `<li><strong>${c.symbol}</strong>: ${c.price_change_1y.toFixed(1)}% growth - ${c.company_name}</li>`).join('')}
                    </ul>
                </div>
                
                <div class="recommendation-card rec-medium">
                    <div class="rec-title"> Strong Momentum (>20% 3M)</div>
                    <ul class="rec-list">
                        ${momentumCompanies.map(c => `<li><strong>${c.symbol}</strong>: ${c.price_change_3m.toFixed(1)}% momentum - ${c.company_name}</li>`).join('')}
                    </ul>
                </div>
            `;
        }

        function populateMarketTrends() {
            const container = document.getElementById('marketTrends');
            if (!container) return;
            
            container.innerHTML = `
                <div style="border-left: 4px solid #10b981; padding-left: 16px; margin-bottom: 16px;">
                    <h4 style="font-weight: 600; margin-bottom: 4px;">AI Infrastructure Boom</h4>
                    <p style="font-size: 0.875rem; color: #64748b;">
                        NVIDIA and AMD leading with exceptional growth, indicating massive AI infrastructure investments requiring specialized logistics.
                    </p>
                </div>
                
                <div style="border-left: 4px solid #3b82f6; padding-left: 16px; margin-bottom: 16px;">
                    <h4 style="font-weight: 600; margin-bottom: 4px;">Cloud Provider Expansion</h4>
                    <p style="font-size: 0.875rem; color: #64748b;">
                        Microsoft, Google, and Meta showing strong performance correlates with data center expansion and increased server rack shipments.
                    </p>
                </div>
                
                <div style="border-left: 4px solid #f59e0b; padding-left: 16px; margin-bottom: 16px;">
                    <h4 style="font-weight: 600; margin-bottom: 4px;">Foundry Dominance</h4>
                    <p style="font-size: 0.875rem; color: #64748b;">
                        TSMC's strong performance confirms foundry model success, creating opportunities for Asia-Pacific logistics routes.
                    </p>
                </div>
                
                <div style="border-left: 4px solid #8b5cf6; padding-left: 16px;">
                    <h4 style="font-weight: 600; margin-bottom: 4px;">Equipment Sector Stability</h4>
                    <p style="font-size: 0.875rem; color: #64748b;">
                        Mixed performance in equipment sector suggests selective opportunities for high-value manufacturing equipment logistics.
                    </p>
                </div>
            `;
        }

        function populateStrategicRecommendations() {
            const container = document.getElementById('strategicRecommendations');
            if (!container) return;
            
            const immediate = companiesData.filter(c => c.logistics_opportunity_score > 60 && c.price_change_1y > 20);
            const strategic = companiesData.filter(c => c.current_price > 200 && c.logistics_opportunity_score > 50);
            const risks = companiesData.filter(c => c.price_change_1y < 0 || c.volatility_30d > 50);
            
            container.innerHTML = `
                <div class="recommendation-card rec-immediate">
                    <div class="rec-title"> Immediate Priorities</div>
                    <ul class="rec-list">
                        ${immediate.map(c => `<li><strong>${c.symbol}</strong> - High opportunity (${c.logistics_opportunity_score}) with strong growth (${c.price_change_1y.toFixed(1)}%)</li>`).join('')}
                    </ul>
                </div>
                
                <div class="recommendation-card rec-strategic">
                    <div class="rec-title"> Strategic Partnerships</div>
                    <ul class="rec-list">
                        ${strategic.map(c => `<li><strong>${c.symbol}</strong> - Large scale operations ($${c.current_price.toFixed(0)}) with solid opportunity (${c.logistics_opportunity_score})</li>`).join('')}
                    </ul>
                </div>
                
                <div class="recommendation-card rec-risk">
                    <div class="rec-title"> Risk Considerations</div>
                    <ul class="rec-list">
                        ${risks.map(c => `<li><strong>${c.symbol}</strong> - ${c.price_change_1y < 0 ? 'Declining performance' : 'High volatility'} (${c.volatility_30d.toFixed(1)}%)</li>`).join('')}
                    </ul>
                </div>
            `;
        }

        // Helper functions
        function getCategoryClass(category) {
            const classMap = {
                'Semiconductor Designer': 'cat-designer',
                'Cloud Provider': 'cat-cloud',
                'Foundry': 'cat-foundry',
                'Equipment': 'cat-equipment',
                'Memory': 'cat-memory',
                'Device Manufacturer': 'cat-device',
                'Semiconductor IDM': 'cat-idm'
            };
            return classMap[category] || 'cat-idm';
        }

        function getScoreClass(score) {
            if (score >= 60) return 'score-high';
            if (score >= 45) return 'score-medium';
            return 'score-low';
        }

        function getTrajectoryClass(trajectory) {
            if (trajectory === 'Accelerating' || trajectory === 'Growing') return 'growth-up';
            if (trajectory === 'Declining') return 'growth-down';
            return 'growth-stable';
        }

        // Initialize the dashboard
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize overview tab by default
            initializeChartsForTab('overview');
        });
    </script>
</body>
</html>

