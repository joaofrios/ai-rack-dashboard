            {symbol: 'AMZN', company_name: 'Amazon.com Inc', current_price: 198.78, price_change_1y: 18.7, price_change_3m: 11.2, volatility_30d: 26.5, logistics_opportunity_score: 58, category: 'Cloud Provider', growth_trajectory: 'Growing'}
